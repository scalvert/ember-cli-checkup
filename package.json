{
  "name": "ember-cli-checkup",
  "version": "0.0.1",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/scalvert/ember-cli-checkup.git",
  "license": "MIT",
  "author": "scalvert <steve.calvert@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "clean": "find lib interfaces node-tests -name '*.js' -type f -delete",
    "lint": "eslint --ext js,ts .",
    "pretest": "yarn build",
    "test": "yarn test:node",
    "test:node": "qunit node-tests/unit",
    "prepublishOnly": "yarn build",
    "postpublish": "yarn clean"
  },
  "dependencies": {
    "capitalize": "^2.0.0",
    "ember-cli-babel": "^7.11.0",
    "globby": "^10.0.1",
    "ora": "^3.0.0",
    "p-map": "^3.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.7.0",
    "@types/capitalize": "^1.0.1",
    "@types/console-ui": "^2.2.0",
    "@types/ember": "^3.0.29",
    "@types/ember-qunit": "^3.4.5",
    "@types/ember-test-helpers": "^1.0.4",
    "@types/ember-testing-helpers": "^0.0.3",
    "@types/ember__test-helpers": "^0.7.7",
    "@types/node": "^12.7.2",
    "@types/qunit": "^2.5.4",
    "@types/resolve": "^0.0.8",
    "@types/rsvp": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "^3.12.0",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-htmlbars": "^3.0.1",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-typescript": "^2.0.2",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.12.0",
    "ember-source-channel-url": "^2.0.1",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-ember": "^6.2.0",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "fixturify-project": "^1.9.0",
    "loader.js": "^4.7.0",
    "prettier": "^1.18.2",
    "qunit-dom": "^0.9.0",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333",
    "typescript-require": "^0.2.10"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
